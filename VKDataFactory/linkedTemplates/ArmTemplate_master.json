{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "VKDataFactory"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"UsingParameter_properties_pipeline1_parameters_createdBy": {
			"type": "string",
			"defaultValue": "Amit"
		},
		"UsingParameter_properties_pipeline1_parameters_logType": {
			"type": "string",
			"defaultValue": "AmitLog"
		},
		"UsingParameter_properties_pipeline1_parameters_Message": {
			"type": "string",
			"defaultValue": "Salary kab aayagi."
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "VKDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"UsingParameter_properties_pipeline1_parameters_createdBy": {
						"value": "[parameters('UsingParameter_properties_pipeline1_parameters_createdBy')]"
					},
					"UsingParameter_properties_pipeline1_parameters_logType": {
						"value": "[parameters('UsingParameter_properties_pipeline1_parameters_logType')]"
					},
					"UsingParameter_properties_pipeline1_parameters_Message": {
						"value": "[parameters('UsingParameter_properties_pipeline1_parameters_Message')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}